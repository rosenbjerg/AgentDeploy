{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ExecutionOptions": {
    "UseWsl": true,
    "Shell": "/bin/sh"
  },
  "DirectoryOptions": {
    "Scripts": "/app/scripts",
    "Tokens": "/app/tokens"
  }
}
