{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ExecutionOptions": {
    "Shell": "bash",
    "ShellFileExtension": ".sh",
    "FileArgumentFormat": "$(ScriptPath)"
  },
  "AgentOptions": {
    "TrustXForwardedHeaders": true,
    "AllowCors": true,
    "RequireHttps": false
  },
  "DirectoryOptions": {
    "Scripts": "./scripts",
    "Tokens": "./tokens"
  }
}
