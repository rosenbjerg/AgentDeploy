{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ExecutionOptions": {
    "Shell": "/bin/sh"
  },
  "DirectoryOptions": {
    "Scripts": "/app/scripts",
    "Tokens": "/app/tokens",
    "Assets": "/app/assets"
  }
}
