{
  "name": "agentd-client",
  "version": "2.0.0",
  "bin": "./src/index.js",
  "pkg": {
    "targets": [ "node8-linux-x64", "node8-win-x64", "node8-macos-x64", "node8-alpine-x64" ],
    "outputPath": "dist"
  },
  "scripts": {
    "build:linux-x64": "nexe src/index.js -t linux-x64-12.16.2 -n agentd-client-linux-x64",
    "build:win-x64": "nexe src/index.js -t windows-x64-12.18.2 -n agentd-client-windows-x64",
    "build:mac-x64": "nexe src/index.js -t mac-x64-12.18.2 -n agentd-client-mac-x64",
    "build:all": "npm run build:linux-x64 && npm run build:win-x64 && npm run build:mac-x64"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^7.1.0",
    "dateformat": "^4.5.1",
    "form-data": "^4.0.0",
    "node-fetch": "^2.6.1",
    "uuid": "^8.3.2",
    "ws": "^7.4.4"
  }
}
